{
    "hero": {
        "content": "The planner for",
        "carouselItems": ["school", "study", "work"],
        "carouselRate": 4000,
        "carouselTextAnimationRate": 100
    },
    "ctaButtonContent": {
        "section1-PostHero": ["Sign up now", "Go to dashboard"]
    },
    "forms": {
        "login": {
            "t": "Login",
            "a": "/auth",
            "f": [
                { "n": "username", "t": "text", "p": "^[A-Za-z0-9\\_]*$" },
                { "n": "password", "t": "password" }
            ]
        },
        "register": {
            "t": "Sign up",
            "a": "/register",
            "f": [
                { "n": "username", "t": "text", "p": "^[A-Za-z0-9\\_]*$" },
                { "n": "email", "t": "text", "p": "^\\w+([\\.-]?\\w+)*@\\w+([\\.-]?\\w+)*(\\.\\w{2,3})+$" },
                { "n": "password", "t": "password" }
            ]
        }
    },
    "navbar": {
        "logoUrl": "imgs/LogoWhite.svg",
        "pages": [
            [
                {
                    "u": "/",
                    "n": "Home"
                },
                {
                    "u": "/login",
                    "n": "Login"
                },
                {
                    "u": "/register",
                    "n": "Sign up"
                }
            ],
            [
                {
                    "u": "/dashboard",
                    "n": "Dashboard"
                },
                {
                    "u": "/settings",
                    "n": "Settings"
                },
                {
                    "u": "/logout",
                    "n": "Logout"
                }
            ]
        ]
    },
    "footer": {
        "content": "Created by Oliver B for a school project"
    }
